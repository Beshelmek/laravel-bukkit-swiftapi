{
    "name": "radic/bukkit-swift-api",
    "description": "SwiftApi PHP API wrapped in a laravel package for remote calls to a minecraft bukkit server.",
    "homepage": "https://github.com/RobinRadic/laravel-bukkit-swiftapi",
    "keywords": ["minecraft", "bukkit", "swiftapi", "laravel"],
    "license": "GNU General Public License version 3 (GPLv3)",
    "authors": [
        {
            "name": "Robin Radic",
            "email": "robin@radic.nl",
            "role": "Package Author/Developer"
        },
        {
            "name": "phybros",
            "homepage": "http://dev.bukkit.org/bukkit-plugins/swiftapi/",
            "role": "SwiftApi Author"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.*"
    },
    "autoload": {
        "psr-0": {
            "Radic\\BukkitSwiftApi": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev"
}
